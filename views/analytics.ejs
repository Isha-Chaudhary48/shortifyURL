<div id="main">
  <h1 id="heading">Analytics</h1>

  <form action="/data" method="post" id="formid">
    <input
      type="text"
      placeholder="Enter shortId eg.H1XdielEke"
      name="shortId"
      id="input"
    />
    <button id="showButton" type="submit">show</button>
  </form>

  <% if (msg) { %>
  <p id="message"><%= msg %></p>
  <% } %>
  <table id="table">
    <tr>
      <th>Orignal Url</th>

      <th>Visit History</th>
      <th>Total Clicks</th>
    </tr>
    <tr>
      <td><%=redirectURL %></td>

      <td>
        <% if (visitHistory.length === 0) { %>
        <li>No visits yet.</li>
        <% } else { %> <% visitHistory.forEach((visit, index) => { %>
        <li>
          Visit <%= index + 1 %>: <%= new Date(visit.timestamp).toLocaleString()
          %>
        </li>
        <% }); %> <% } %>
      </td>
      <td><%= totalClicks%></td>
    </tr>
  </table>
</div>
